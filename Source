		function Functions:CreateBox(Text, PlaceholderText, callback)
			Text = Text or "NIL"
			PlaceholderText = PlaceholderText or "NIL"
			callback = callback or function() end
			local TextBoxFrame = Instance.new("ImageLabel")
			local Box = Instance.new("TextBox")
			
			TextBoxFrame.Name = "TextBoxFrame"
			TextBoxFrame.Parent = PLAYERTab
			TextBoxFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			TextBoxFrame.BackgroundTransparency = 1.000
			TextBoxFrame.BorderSizePixel = 0
			TextBoxFrame.Position = UDim2.new(0.0293040294, 0, 0.00398406386, 0)
			TextBoxFrame.Size = UDim2.new(0, 256, 0, 24)
			TextBoxFrame.ZIndex = 2
