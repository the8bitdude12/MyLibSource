pcall(function()
	if game.CoreGui:FindFirstChild("P|O|R|N|H|U|B") then
		game.CoreGui:FindFirstChild("P|O|R|N|H|U|B"):Destroy()
	end
end)

function ShiftGui(Frame, List)
	for _, v in pairs(List:GetChildren()) do
		v.Visible = false
		Frame.Visible = true
	end
end

local Library = {}
	
function Library:CreateWindow(Name)
