		PLAYER.MouseButton1Up:Connect(function()
			ShiftGui(PLAYERTab, TabList)
		end)
		
		local Functions = {}
		
		function Functions:CreateButton(Text, callback)
			Text = Text or "NIL"
			callback = callback or function() end
			local ButtonFrame = Instance.new("ImageLabel")
			local Button = Instance.new("TextButton")
			
			ButtonFrame.Name = "ButtonFrame"
			ButtonFrame.Parent = PLAYERTab
			ButtonFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			ButtonFrame.BackgroundTransparency = 1.000
			ButtonFrame.BorderSizePixel = 0
			ButtonFrame.Position = UDim2.new(0.0293040294, 0, 0.00398406386, 0)
			ButtonFrame.Size = UDim2.new(0, 256, 0, 24)
			ButtonFrame.ZIndex = 2
			ButtonFrame.Image = "rbxassetid://3570695787"
			ButtonFrame.ImageColor3 = Color3.fromRGB(91, 91, 91)
			ButtonFrame.ScaleType = Enum.ScaleType.Slice
			ButtonFrame.SliceCenter = Rect.new(100, 100, 100, 100)
			ButtonFrame.SliceScale = 0.050
			
			Button.Name = "Button"
			Button.Parent = ButtonFrame
			Button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Button.BackgroundTransparency = 1.000
			Button.BorderSizePixel = 0
			Button.Position = UDim2.new(0.00390625, 0, 0.0833333358, 0)
			Button.Size = UDim2.new(0, 256, 0, 22)
			Button.ZIndex = 2
			Button.Font = Enum.Font.GothamBold
			Button.Text = Text
			Button.TextColor3 = Color3.fromRGB(255, 255, 255)
			Button.TextScaled = true
			Button.TextSize = 14.000
			Button.TextWrapped = true
			PLAYERTab.CanvasSize = UDim2.new(0,0,0,(#PLAYERTab:GetChildren()*25)+50)
			Button.MouseButton1Up:Connect(function()
				pcall(callback)
			end)
		end
		
		function Functions:CreateToggle(Text, state, callback)
			Text = Text or "NIL"
			state = state or false
			callback = callback or function() end
			local ToggleFrame = Instance.new("ImageLabel")
			local ToggleButton = Instance.new("TextButton")
			local Color = Instance.new("Frame")
			local ToggleText = Instance.new("TextLabel")
			
			ToggleFrame.Name = "ToggleFrame"
			ToggleFrame.Parent = PLAYERTab
			ToggleFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			ToggleFrame.BackgroundTransparency = 1.000
			ToggleFrame.BorderSizePixel = 0
			ToggleFrame.Position = UDim2.new(0.0293040294, 0, 0.00398406386, 0)
			ToggleFrame.Size = UDim2.new(0, 256, 0, 24)
			ToggleFrame.ZIndex = 2
			ToggleFrame.Image = "rbxassetid://3570695787"
			ToggleFrame.ImageColor3 = Color3.fromRGB(91, 91, 91)
			ToggleFrame.ScaleType = Enum.ScaleType.Slice
			ToggleFrame.SliceCenter = Rect.new(100, 100, 100, 100)
			ToggleFrame.SliceScale = 0.050
			
			ToggleButton.Name = "ToggleButton"
			ToggleButton.Parent = ToggleFrame
			ToggleButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			ToggleButton.BackgroundTransparency = 1.000
			ToggleButton.BorderSizePixel = 0
			ToggleButton.Position = UDim2.new(0.00390625, 0, 0.0833333358, 0)
			ToggleButton.Size = UDim2.new(0, 256, 0, 22)
			ToggleButton.ZIndex = 2
			ToggleButton.Font = Enum.Font.GothamBold
			ToggleButton.Text = ""
			ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
			ToggleButton.TextScaled = true
			ToggleButton.TextSize = 14.000
			ToggleButton.TextWrapped = true
			
			Color.Name = "Color"
			Color.Parent = ToggleFrame
			Color.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
			Color.BorderSizePixel = 0
			Color.Position = UDim2.new(0.03515625, 0, 0.0833333358, 0)
			Color.Size = UDim2.new(0, 20, 0, 20)
			Color.ZIndex = 2
			
			ToggleText.Name = "ToggleText"
			ToggleText.Parent = ToggleFrame
			ToggleText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			ToggleText.BackgroundTransparency = 1.000
			ToggleText.BorderSizePixel = 0
			ToggleText.Position = UDim2.new(0.16796875, 0, 0, 0)
			ToggleText.Size = UDim2.new(0, 213, 0, 24)
			ToggleText.ZIndex = 2
			ToggleText.Font = Enum.Font.GothamBold
			ToggleText.Text = Text
			ToggleText.TextColor3 = Color3.fromRGB(255, 255, 255)
			ToggleText.TextScaled = true
			ToggleText.TextSize = 14.000
			ToggleText.TextWrapped = true
			PLAYERTab.CanvasSize = UDim2.new(0,0,0,(#PLAYERTab:GetChildren()*25)+50)
			
			if state then
				Color.BackgroundColor3 = Color3.new(85, 255, 0)
			else
				Color.BackgroundColor3 = Color3.new(255, 0, 0)
			end
			local TweenService = game:GetService("TweenService")
			local transitionInfo = TweenInfo.new(0.5, Enum.EasingStyle.Back)
			
			local function FireToggle()
				state = not state
				local On = TweenService:Create(ToggleButton, transitionInfo, {BackgroundColor3 = Color3.new(85/255, 255/255, 0/255)})
				local Off = TweenService:Create(ToggleButton, transitionInfo, {BackgroundColor3 = Color3.new(255/255, 0/255, 0/255)})
				if state then
					On:Play()
					pcall(callback, state)
				else
					Off:Play()
					pcall(callback, state)
				end
			end
			ToggleButton.MouseButton1Up:Connect(function()
				FireToggle()
			end)
		end
		
