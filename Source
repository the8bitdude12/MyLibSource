	local Tabs = {}
	
	function Tabs:CreateTab(Name)
		local PLAYER = Instance.new("TextButton")
		local TabButtonFrame = Instance.new("ImageLabel")
		local PLAYERTab = Instance.new("ScrollingFrame")
		local UIGridLayout_2 = Instance.new("UIGridLayout")
		
		PLAYER.Name = "PLAYER"
		PLAYER.Parent = TabButtonList
		PLAYER.BackgroundColor3 = Color3.fromRGB(67, 67, 67)
		PLAYER.BackgroundTransparency = 1.000
		PLAYER.BorderSizePixel = 0
		PLAYER.Position = UDim2.new(0.0329670347, 0, 0.200000003, 0)
		PLAYER.Size = UDim2.new(0, 69, 0, 24)
		PLAYER.ZIndex = 4
